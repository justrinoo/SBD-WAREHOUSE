<%- include('../partials/header.ejs') %>
<div class="container py-5 pb-5">
    <div class="row">
        <div class="col-md-6 justify-content-center align-items-center mx-auto">
            <div class="card">
                <div class="card-header">
                    <h3>Detail Penerimaan</h3>
                </div>
                <div class="card-body d-flex flex-column">
                    <div class="d-flex align-items-center">
                        <span>Tanggal : </span>
                        <h6 class="mt-2 mx-2"><%= data.Date_Time; %></h6>
                    </div>
                    <div class="d-flex align-items-center">
                        <span>Gudang : </span>
                        <h6 class="mt-2 mx-2"><%= data.warehouse_name; %></h6>
                    </div>
                    <div class="d-flex align-items-center">
                        <span>Kuantity Produk : </span>
                        <h6 class="mt-2 mx-2"><%= data.Kuantity_Product; %></h6>
                    </div>
                    <div class="d-flex align-items-center">
                        <span>Note : </span>
                        <h6 class="mt-2 mx-2"><%= data.Note; %></h6>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<%- include('../partials/footer.ejs') %>
